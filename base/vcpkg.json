      {
        "name": "redis-plus-plus",
        "platform": "!arm64"
      },
      {
        "name": "re",
        "platform": "!windows"
      },
      {
        "name": "baresip",
        "platform": "!windows"
      },
      {
        "name": "libmp4"
      }
    ]
