    "libjpeg-turbo",
    "openh264",
    "ffmpeg",
    "bigint",
    "boost-math",
    "boost-system",
    "boost-thread",
    "boost-filesystem",
    "boost-serialization",
    "boost-log",
    "boost-chrono",
    "boost-test",
    "boost-iostreams",
    "boost-dll",
    "nu-book-zxing-cpp",
    "liblzma",
    "bzip2",
    "zlib",
    "sfml",
    {
      "name": "glib",
      "default-features": false,
      "features": [
        "libmount"
