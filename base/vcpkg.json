{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "apra-pipes-cuda",
  "version": "0.0.1",
  "builtin-baseline": "725267a6265f1ce71d8ce7ffa60de4bb321e0669",
  "dependencies": [
    {
      "name": "opencv4",
      "default-features": false,
      "features": ["contrib", "cuda", "cudnn", "dnn", "jpeg", "nonfree", "png", "tiff", "webp" ]
    },
    "ffmpeg",
    "bigint",
    "boost-math",
    "boost-system",
    "boost-thread",
    "boost-filesystem",
    "boost-serialization",
    "boost-log",
    "boost-chrono",
    "boost-test",
    "boost-iostreams",
    "boost-dll",
    "nu-book-zxing-cpp",
    "liblzma",
    "bzip2",
    "zlib",
    "sfml",
    {
      "name": "glib",
      "default-features": false,
      "features": ["libmount"],
      "platform": "(linux & x64)",
      "$reason" : "skip linux:arm64 and windows"
    },
    {
      "name": "glib",
      "default-features": true,
      "platform": "windows"
    },
    {
      "name": "hiredis",
      "platform": "!arm64"
    },
    {
      "name": "redis-plus-plus",
      "platform": "!arm64"
    },
    {
      "name": "gstreamer",
      "features": ["plugins-good","plugins-bad","plugins-ugly" ],
      "platform": "!arm64",
      "$reason": "current vcpkg system does not build gstreamer for arm linux, we build our own"
    },
    {
      "name":"gst-rtsp-server",
      "platform": "!arm64",
      "$reason": "current vcpkg system does not build gstreamer for arm linux, we build our own"
    }
  ]
}