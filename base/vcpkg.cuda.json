{
  "$schema": "https://raw.githubusercontent.com/microsoft/vcpkg/master/scripts/vcpkg.schema.json",
  "name": "apra-pipes-x64-cuda",
  "version": "0.0.1",
  "builtin-baseline": "664f4fa445147be58f632bbe8c8ee221a124552c",
  "dependencies": [
    {
      "name": "opencv4",
      "default-features": false,
      "features": ["cuda","cudnn","nonfree","png","jpeg","tiff","dnn","webp","contrib" ]
    },
    "ffmpeg",
    "bigint",
    "boost-math",
    "boost-system",
    "boost-thread",
    "boost-filesystem",
    "boost-serialization",
    "boost-log",
    "boost-chrono",
    "boost-test",
    "boost-iostreams",
    "boost-dll",
    "nu-book-zxing-cpp",
    "liblzma",
    {
      "name": "glib",
      "default-features": false,
      "features": ["libmount"],
      "platform": "!windows"
    },
    {
      "name": "glib",
      "default-features": true,
      "platform": "!linux"
    },
    "bzip2",
    "zlib",
    "hiredis",
    "redis-plus-plus",
    "sfml",
    {
      "name": "gstreamer",
      "version>=": "1.19.2#3",
      "features": ["plugins-good","plugins-bad","plugins-ugly" ]
    },
    "gst-rtsp-server"
  ]
}